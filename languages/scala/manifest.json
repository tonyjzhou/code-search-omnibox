{
  "name": "Scala Code Search",
  "description": "Type-ahead omnibox search for all symbol-indexed repos in Phabricator",
  "author": "Tony Zhou",
  "manifest_version": 2,
  "version": "1",

  "permissions": [
    "tabs",
    "https://phabricator.twitter.biz/*",
    "https://phabricator-staging.twitter.biz/*"
  ],

  "omnibox": {
    "keyword": "css"
  },

  "icons": {
    "16": "twitter-128.png",
    "48": "twitter-128.png",
    "128": "twitter-128.png"
  },

  "browser_action": {
    "default_icon": "twitter-128.png"
  },

  "background": {
    "scripts": [
      "3rdparty/jquery-3.1.0.js",
      "language.js",
      "background.js"
    ]
  }
}
